{
    "displayName": "High CPU Usage Alert",
    "combiner": "OR",
    "conditions": [
      {
        "displayName": "Instance CPU usage",
        "conditionThreshold": {
          "filter": "metric.type=\"compute.googleapis.com/instance/cpu/utilization\" AND resource.type=\"gce_instance\"",
          "comparison": "COMPARISON_GT",
          "thresholdValue": 0.8,
          "duration": "300s",
          "trigger": {
            "count": 1
          }
        }
      }
    ],
    "notificationChannels": [
      "projects/nao-ai-project/notificationChannels/YOUR_CHANNEL_ID"
    ],
    "enabled": true
  }
  